package Website;

import java.sql.Date;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import lombok.Builder;


// @Entity : đanh dấu đây là một 1 Entity trong database
@Entity
@lombok
@Builder
public class Book {
	@Id
	private String authId;
	private String name;
	private Date dateOfBirth;
	
	
}
