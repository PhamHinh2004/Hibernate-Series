//	private Set<NhanVien> danhSachNhanVien = new HashSet<NhanVien>();

	public CuocHop() {
		super();
	}

//	public CuocHop(int id, String tenCuocHop, String diaChi, Date thoiGian, Set<NhanVien> danhSachNhanVien) {
//		super();
//		this.id = id;
